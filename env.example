# Copy this file to `.env.local` (Next.js), `.env` (scripts) and customize.
# Fill in every placeholder before deploying.

############################################
# Public Next.js runtime (exposed to client)
############################################
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=public-anon-key
NEXT_PUBLIC_SUPABASE_SERVICE_ROLE_KEY=

NEXT_PUBLIC_SOCKET_URL=http://localhost:4001
NEXT_PUBLIC_SOCKET_PATH=/socket.io
NEXT_PUBLIC_LOBBY_HTTP_FALLBACK=false

NEXT_PUBLIC_SERVER_URL=http://localhost:3001
NEXT_PUBLIC_ADMIN_WALLET=

NEXT_PUBLIC_SOLANA_CLUSTER=mainnet
NEXT_PUBLIC_SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

############################################
# Supabase service credentials (server only)
############################################
SUPABASE_URL=${NEXT_PUBLIC_SUPABASE_URL}
SUPABASE_SERVICE_KEY=
SUPABASE_SERVICE_ROLE_KEY=${SUPABASE_SERVICE_KEY}

# Optional aliases supported by `lib/supabase-admin`
NEXT_SUPABASE_URL=${NEXT_PUBLIC_SUPABASE_URL}
NEXT_SUPABASE_SERVICE_ROLE_KEY=${SUPABASE_SERVICE_ROLE_KEY}
NEXT_SUPABASE_SERVICE_KEY=${SUPABASE_SERVICE_KEY}

############################################
# Solana / escrow wallets
############################################
ADMIN_WALLET=
PAYOUT_SERVER_SECRET=
HOUSE_CUT_PERCENTAGE=0.04

ESCROW_WALLET_A_PUBLIC_KEY=
ESCROW_WALLET_A_PRIVATE_KEY=
ESCROW_WALLET_B_PUBLIC_KEY=
ESCROW_WALLET_B_PRIVATE_KEY=

# Optional helpers if you mirror docs/env-setup-guide.md
SOLANA_PRIVATE_KEY_ESCROW_A=
SOLANA_PRIVATE_KEY_ESCROW_B=

############################################
# Socket / backend services
############################################
TRACK_LOBBY_PRESENCE=true
SUPABASE_LOBBY_PERSIST=false
LOG_LEVEL=info

# `server/socket-server.ts` defaults to 4001, `server/index.js` defaults to 3001.
# Override before starting each process if you need different ports.
PORT=4001

############################################
# Stress-test script overrides (optional)
############################################
STRESS_SOCKET_URL=${NEXT_PUBLIC_SOCKET_URL}
STRESS_SOCKET_PATH=${NEXT_PUBLIC_SOCKET_PATH}
STRESS_LOBBY_ID=free-test-match
STRESS_CLIENTS=3
STRESS_READY_DELAY_MS=3000
STRESS_RUNTIME_MS=60000
STRESS_STATE_INTERVAL_MS=200
STRESS_USE_POLLING=false

